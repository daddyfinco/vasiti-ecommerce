                {{/if}}
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!--/header_top-->
    <!--header-middle-->
    <div class="header-middle">
      <div class="container">
        <div class="row">
          <div class="col-sm-4">
            <div class="logo pull-left">
              <a href="/"><img src="https://vasiti.com/img/site/logo.png" alt="aStore Logo">
                <!--span a
| Store
i.fa.fa-shopping-cart-->
              </a></div>
          </div>
          <div class="col-sm-8"></div>
        </div>
      </div>
    </div>
    <!--/header-middle-->
    <!--header-bottom-->
    <div class="header-bottom">
      <div class="container">
        <div class="row">
          <div class="col-sm-9">
            <div class="navbar-header"><button type="button" data-toggle="collapse" data-target=".navbar-collapse" class="navbar-toggle"><span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></button></div>
            <div class="mainmenu pull-left">
              <ul class="nav navbar-nav collapse navbar-collapse">
                <li><a href="/" class="">Home</a></li>
                <li><a href="/admin/products" class="">All Product</a></li>
                <li><a href="/admin/products/add" class="">Add Product</a></li>
              </ul>
            </div>
          </div>
          <div class="col-sm-3">
          </div>
        </div>
      </div>
    </div>
