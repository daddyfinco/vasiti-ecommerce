    <!--header_top-->
    <div class="header_top">
      <div class="container">
        <div class="row">
          <div class="col-sm-6">
            <div class="contactinfo">
              <ul class="nav nav-pills">
                <li><a href="#"><i class="fa fa-phone"></i> +2348146439559</a></li>
                <li><a href="mailto:info@astore.com"><i class="fa fa-envelope"></i> info@astore.com</a></li>
              </ul>
            </div>
          </div>
          <div class="col-sm-6">
            <div class="shop-menu pull-right">
              <ul class="nav navbar-nav">{{var orderCus = 'Account'}}{{var link = '/sign-in'}}
                {{#if customer}}
                  {{var orderCus=customer.FullName}}{{var link='/usr/'}}
                {{/if}}
                <li><a href="{{link}}"><i class="fa fa-user"></i>{{orderCus}}</a></li>
                <li><a href="/cart"><i class="fa fa-shopping-cart"></i> Cart</a></li>
                <!--li-->
                <!--    a(href='/checkout')-->
                <!--        i.fa.fa-money-->
                <!--        |  Checkout-->{{#if customer}}
